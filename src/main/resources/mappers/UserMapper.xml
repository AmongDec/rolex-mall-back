<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.montnets.mtcsto.Mapper.login.UserMapper">
    <update id="updateByPhone">
        update user set u_pwd = #{pass} where u_phone = ${phone}
    </update>
</mapper>
